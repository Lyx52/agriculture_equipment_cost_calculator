    "vue-chartjs": "^5.3.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/tsconfig": "^0.5.1",
    "npm-run-all2": "^6.2.3",
    "sass-embedded": "^1.80.3",
    "typescript": "~5.5.4",
